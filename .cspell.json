  "allowCompoundWords": true,
  "caseSensitive": false,
  "flagWords": [],
  "failFast": false,
  "showSuggestions": true,
  "validateDirectives": true,
  "patterns": [
    {
      "name": "Markdown links",
      "pattern": "\\[([^\\]]+)\\]\\(([^\\)]+)\\)",
      "description": "Ignore markdown links"
    },
    {
      "name": "URLs",
      "pattern": "https?://\\S+",
      "description": "Ignore URLs"
    },
    {
      "name": "Email addresses",
      "pattern": "\\b[A-Za-z0-9._%+-]+@[A-Za-z0-9.-]+\\.[A-Z|a-z]{2,}\\b",
      "description": "Ignore email addresses"
    },
    {
      "name": "Code blocks",
      "pattern": "```[\\s\\S]*?```",
      "description": "Ignore code blocks"
    },
    {
      "name": "Inline code",
      "pattern": "`[^`]+`",
      "description": "Ignore inline code"
    },
    {
      "name": "HTML tags",
      "pattern": "<[^>]+>",
      "description": "Ignore HTML tags"
    },
    {
      "name": "Hex colors",
      "pattern": "#[0-9a-fA-F]{3,8}",
      "description": "Ignore hex colors"
    },
    {
      "name": "Version numbers",
      "pattern": "\\b\\d+\\.\\d+(\\.\\d+)?([.-]\\w+)?\\b",
      "description": "Ignore version numbers"
    },
    {
      "name": "File extensions",
      "pattern": "\\.[a-zA-Z0-9]+$",
      "description": "Ignore file extensions"
    }
  ]
}